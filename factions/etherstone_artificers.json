  "rules": {
    "paired_ambush": {
      "name": "Paired Ambush",
      "description": "This unit and a paired _Infantry_ unit may be placed in _Ambush Reserves_ before the game begins. When this unit arrives from _Ambush_, setup the paired unit within 3\" of it.",
      "points": 10
    },
    "legend_drillmaster": {"name": "Drillmaster", "description": "This model and friendly units within 12\" gain +3\" to their long range when shooting.", "points": 10},
    "suit": {"name": "Skybound Suit", "description": "This model gains +3\" Movement, Wounds(+1) and may ignore terrain and other models without the Fly special rule when moving.", "points": 15},
    "clockwork_companion": {"name": "Clockwork Companion", "description": "This model and its unit re-rolls failed hit rolls of 9+ when shooting.", "points": 5},
    "opportunist": {
      "name": "Opportunist",
      "description": "When an enemy model takes any unsaved hits by this models melee attacks, you can push that model 1“ directly away from this model.",
      "points": 5
    },
    "brew": {
      "name": "Dwarven Brew",
      "description": "When this unit is activated, choose a Beer for this unit or a friendly _Infantry_ unit within 6\" to drink:\n* Clockwork Malt: Roll a die when a model in this unit loses a wound. On a value of 1 or 2 that wound is not lost.\n* Berserker Brew: When this unit fights, hit rolls of 1 or 2 generate an additional attack.\n* Ol’ Classic: One model in the unit immediately heals D3 wounds.",
      "points": [15]
    }
  },
  "powers": {
    "ancestral_wrath": {"name": "Ancestral Wrath", "description": "Target enemy unit within 12\" takes 4 hits with AP(4).", "charge": 5},
    "runic_barrier": {
      "name": "Runic Barrier",
      "description": "Target friendly unit within 24\" rolls a D10 each time that unit is hit until the end of the turn. On a 2 or less, that hit is ignored.",
      "charge": 6
    },
    "blink": {"name": "Blink", "description": "Target friendly unit within 12\" may move up to 12\" ignoring all models and terrain. This must end outside of 12\" of any enemy units.", "charge": 7}
  },
  "weapons": {
    "devouring_maw": {"name": "Devouring Maw", "ap": 6, "attacks": 1, "short": "Melee", "rules": [{"id": "deadly", "x": 3}]},
    "napalm_breath": {"name": "Napalm Breath", "rules": ["flame", "lethal"], "attacks": 3, "ap": 4, "medium": 12, "short": 6},
    "alchemical_grenade": {
      "name": "Chemical Bomb",
      "profiles": [
        {"name": "Unstable", "attacks": "1", "ap": 5, "short": 6, "rules": ["grenade", "overcharge"]},
        {"name": "Flash", "attacks": "1", "short": 6, "rules": ["grenade", "stun", {"id": "blast", "x": 3}]},
        {"name": "Siphon", "attacks": "1", "short": 6, "rules": ["grenade", "leeching", {"id": "blast", "x": 3}]}
      ]
    },
    "welding_beam": {"name": "Welding Beam", "attacks": 1, "ap": 5, "short": 6},
    "rivet_pistol": {"name": "Rivet Pistol", "attacks": 3, "short": 6, "ap": 2, "rules": ["lethal", "pistol"]}
  },
  "strategies": [
    {
      "name": "Stubborn",
      "phase": "activation",
      "description": "Use before activating a unit. That unit may immediately remove up to 2 Shock.",
      "flavor": "Standing steadfast like their mountain halls.",
      "cost": 1
    },
    {
      "name": "Shieldwall",
      "phase": "activation",
      "description": "Use when an _Infanty_ unit with Shields is charged or targeted by a shooting attack. That unit gains +1 Save until end of turn.",
      "flavor": "Forming a shieldwall, dwarven formations are able to hold objectives against all odds.",
      "cost": 2
    },
    {
      "phase": "activation",
      "name": "Final Stand",
      "description": "Use when a friendly unit would be destroyed. If that unit has not activated yet this round, it may perform one shoot or fight action before it is destroyed.",
      "flavor": "A dwarf fights to the last breath.",
      "cost": 1
    }
  ],
  "relics": [
